property nukleiR05C ${nuklei:directory("target/nukleus-itests").controlCapacity(4096, 4096)}

property controlR05C ${nukleiR05C.control("security")}

connect "agrona://stream/bidirectional"
        option agrona:reader ${agrona:broadcastReceiver(controlR05C.controller)}
        option agrona:writer ${agrona:manyToOneWriter(controlR05C.nukleus)}

connected

write [0x00 0x00 0x00 0x03]
write ${controlR05C.nextCorrelationId()}
write [0x05] "role1"
write [0x05] "role2"
write [0x05] "role3"
write [0x05] "role4"
write [0x05] "role5"
write [0x05] "role6"
write [0x05] "role7"
write [0x05] "role8"
write [0x05] "role9"
write [0x05] "role10"
write [0x05] "role11"
write [0x05] "role12"
write [0x05] "role13"
write [0x05] "role14"
write [0x05] "role15"
write [0x05] "role16"
write [0x05] "role17"
write [0x05] "role18"
write [0x05] "role19"
write [0x05] "role20"
write [0x05] "role21"
write [0x05] "role22"
write [0x05] "role23"
write [0x05] "role24"
write [0x05] "role25"
write [0x05] "role26"
write [0x05] "role27"
write [0x05] "role28"
write [0x05] "role29"
