read [0x05] "role43"
read [0x05] "role44"
read [0x05] "role45"
read [0x05] "role46"
read [0x05] "role47"
read [0x05] "role48"
read [0x05] "role49TooMany"


write [0x40 0x00 0x00 0x00]
write ${correlationIdR05N}
write flush

close
closed
