property nukleiR05N ${nuklei:directory("target/nukleus-itests").controlCapacity(4096, 4096)}

property controlR05N ${nukleiR05N.controlNew("security")}

connect "agrona://stream/bidirectional"
        option agrona:reader ${agrona:manyToOneReader(controlR05N.nukleus)}
        option agrona:writer ${agrona:broadcastTransmitter(controlR05N.controller)}

connected

read [0x00 0x00 0x00 0x03]
read ([0..8]:correlationIdR05N)
read [0x05] "role1"
read [0x05] "role2"
read [0x05] "role3"
read [0x05] "role4"
read [0x05] "role5"
read [0x05] "role6"
read [0x05] "role7"
read [0x05] "role8"
read [0x05] "role9"
read [0x05] "role10"
read [0x05] "role11"
read [0x05] "role12"
read [0x05] "role13"
read [0x05] "role14"
read [0x05] "role15"
read [0x05] "role16"
read [0x05] "role17"
read [0x05] "role18"
read [0x05] "role19"
read [0x05] "role20"
read [0x05] "role21"
read [0x05] "role22"
read [0x05] "role23"
read [0x05] "role24"
read [0x05] "role25"
read [0x05] "role26"
read [0x05] "role27"
read [0x05] "role28"
read [0x05] "role29"
read [0x05] "role30"
read [0x05] "role31"
read [0x05] "role32"
read [0x05] "role33"
read [0x05] "role34"
read [0x05] "role35"
read [0x05] "role36"
read [0x05] "role37"
read [0x05] "role38"
read [0x05] "role39"
read [0x05] "role40"
read [0x05] "role41"
